{
  "bundles": [{
    "src": "src/poly-loader.js",
    "dest": "dist/poly-loader.js",
    "moduleName": "polyLoader",
    "format": "iife"
  }, {
    "src": "src/backed.js",
    "dest": "dist/backed.js",
    "moduleName": "Backed",
    "format": "iife"
  }, {
    "src": "src/backed.js",
    "dest": "dist/backed.js",
    "moduleName": "Backed",
    "format": "cjs",
    "plugins": {
      "babel": {
        "presets": [[
          "es2015", {
            "modules": false
          }
        ]],
        "plugins": [
          "external-helpers"
        ]
      }
    }
  }, {
    "src": "src/backed.js",
    "dest": "dist/backed.js",
    "moduleName": "Backed",
    "format": "es",
    "plugins": {
      "babel": {
        "presets": ["es2016"]
      }
    }
  }]
}
