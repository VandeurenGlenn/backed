{
  "name": "backed",
  "version": "0.10.0-beta.2",
  "description": "Small web framework for quick app & component development",
  "main": "backed.js",
  "scripts": {
    "prepublish": "esdoc && sh scripts/release.sh && sh scripts/gh-pages.sh",
    "postpublish": "sh scripts/clean.sh",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "flat": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vandeurenglenn/backed.git"
  },
  "author": "Glenn Vadeuren <vandeurenglenn@gmail.com>",
  "license": "CC-BY-NC-ND-4.0",
  "bugs": {
    "url": "https://github.com/vandeurenglenn/backed/issues"
  },
  "homepage": "https://github.com/vandeurenglenn/backed#readme",
  "devDependencies": {
    "babel-preset-es2016": "^6.22.0",
    "custom-app-layout": "^0.9.0-preview",
    "custom-docs": "^0.9.0",
    "esdoc": "^1.0.1",
    "eslint": "^3.15.0",
    "eslint-config-google": "^0.7.1",
    "uglify-es": "^3.0.28"
  },
  "dependencies": {
    "lit-html": "^0.7.1"
  },
  "backed": {
    "bundles": [
      {
        "src": "./src/poly-loader.js",
        "dest": "poly-loader.js",
        "moduleName": "polyLoader",
        "format": "es",
        "plugins": {
          "babel": {
            "presets": [
              "es2016"
            ]
          }
        }
      },
      {
        "src": "./src/mixins/lit-mixin.js",
        "dest": "mixins/lit-mixin.js",
        "moduleName": "LitMixin",
        "format": "es",
        "plugins": {
          "babel": {
            "presets": [
              "es2016"
            ]
          }
        }
      },
      {
        "src": "./src/mixins/property-mixin.js",
        "dest": "mixins/property-mixin.js",
        "moduleName": "PropertyMixin",
        "format": "es",
        "plugins": {
          "babel": {
            "presets": [
              "es2016"
            ]
          }
        }
      },
      {
        "src": "./src/mixins/css-mixin.js",
        "dest": "mixins/css-mixin.js",
        "moduleName": "CSSMixin",
        "format": "es",
        "plugins": {
          "babel": {
            "presets": [
              "es2016"
            ]
          }
        }
      },
      {
        "src": "./src/backed.js",
        "dest": "backed.js",
        "moduleName": "Backed",
        "format": "es",
        "plugins": {
          "babel": {
            "presets": [
              "es2016"
            ]
          }
        }
      },
      {
        "src": "./src/utils.js",
        "dest": "utils.js",
        "moduleName": "BackedUtils",
        "format": "es",
        "plugins": {
          "babel": {
            "presets": [
              "es2016"
            ]
          }
        }
      }
    ]
  },
  "esdoc": {
    "source": "src/",
    "destination": "docs/"
  },
  "resolutions": {
    "babel-traverse": "6.26.0",
    "babylon": "6.18.0",
    "globals": "9.18.0",
    "strip-ansi": "4.0.0",
    "ansi-regex": "3.0.0",
    "backed": "0.9.3",
    "cheerio": "0.22.0",
    "minimist": "1.2.0",
    "entities": "1.1.1",
    "htmlparser2": "3.9.2",
    "domutils": "1.6.2",
    "domelementtype": "1.3.0",
    "domhandler": "2.4.1",
    "readable-stream": "2.3.3",
    "isarray": "1.0.0",
    "string_decoder": "1.0.3",
    "acorn": "5.1.2",
    "esprima": "4.0.0",
    "ajv": "5.2.3",
    "boom": "5.2.0",
    "string-width": "2.1.1",
    "is-fullwidth-code-point": "2.0.0",
    "custom-app-layout": "0.9.0-preview.5",
    "lit-html": "0.7.1"
  }
}
