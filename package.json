  "bugs": {
    "url": "https://github.com/vandeurenglenn/backed/issues"
  },
  "homepage": "https://github.com/vandeurenglenn/backed#readme",
  "esdoc": {
    "source": "src/",
    "destination": "docs/"
  },
  "resolutions": {
