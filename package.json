{
  "name": "backed",
  "version": "0.9.0-beta",
  "description": "Small web framework for quick app & component development",
  "main": "backed.js",
  "scripts": {
    "prepublish": "esdoc && sh scripts/release.sh && sh scripts/gh-pages.sh",
    "postpublish": "sh scripts/clean.sh",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "flat": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vandeurenglenn/backed.git"
  },
  "author": "Glenn Vadeuren <vandeurenglenn@gmail.com>",
  "license": "CC-BY-NC-ND-4.0",
  "bugs": {
    "url": "https://github.com/vandeurenglenn/backed/issues"
  },
  "homepage": "https://github.com/vandeurenglenn/backed#readme",
  "devDependencies": {
    "babel-preset-es2016": "^6.22.0",
    "custom-app-layout": "^0.9.0-preview",
    "esdoc": "^1.0.1",
    "eslint": "^3.15.0",
    "eslint-config-google": "^0.7.1",
    "uglify-es": "^3.0.28"
  },
  "dependencies": {
    "custom-docs": "^0.9.0",
    "lit-html": "^0.5.0",
    "lodash-es": "^4.17.4"
  },
  "resolutions": {
    "debug": "2.6.3",
    "jsesc": "1.3.0",
    "estraverse": "4.2.0",
    "acorn": "5.1.2",
    "string-width": "2.1.1",
    "is-fullwidth-code-point": "2.0.0",
    "commander": "2.11.0",
    "source-map": "0.5.7",
    "babel-runtime": "6.26.0",
    "regenerator-runtime": "0.11.0",
    "babel-traverse": "6.26.0",
    "babel-types": "6.23.0",
    "babylon": "6.18.0",
    "babel-code-frame": "6.26.0",
    "globals": "9.18.0",
    "js-tokens": "3.0.2",
    "to-fast-properties": "1.0.3",
    "mime-types": "2.1.16",
    "mime-db": "1.29.0",
    "babel-generator": "6.24.0",
    "detect-indent": "4.0.0",
    "repeating": "2.0.1",
    "minimist": "1.2.0",
    "readable-stream": "2.3.3",
    "isarray": "1.0.0",
    "string_decoder": "1.0.3",
    "esprima": "3.1.3",
    "ajv": "4.11.8",
    "cheerio": "0.22.0",
    "entities": "1.1.1",
    "htmlparser2": "3.9.2",
    "domutils": "1.6.2",
    "domelementtype": "1.3.0",
    "domhandler": "2.4.1",
    "assert-plus": "1.0.0",
    "lodash": "4.17.4",
    "custom-app-layout": "0.9.0-preview.2",
    "strip-ansi": "4.0.0",
    "ansi-regex": "3.0.0"
  },
  "backed": {
    "bundles": [
      {
        "src": "./src/poly-loader.js",
        "dest": "poly-loader.js",
        "moduleName": "polyLoader",
        "format": "es",
        "plugins": {
          "babel": {
            "presets": [
              "es2016"
            ]
          }
        }
      },
      {
        "src": "./src/mixins/lit-mixin.js",
        "dest": "mixins/lit-mixin.js",
        "moduleName": "LitMixin",
        "format": "es",
        "plugins": {
          "babel": {
            "presets": [
              "es2016"
            ]
          }
        }
      },
      {
        "src": "./src/mixins/property-mixin.js",
        "dest": "mixins/property-mixin.js",
        "moduleName": "PropertyMixin",
        "format": "es",
        "plugins": {
          "babel": {
            "presets": [
              "es2016"
            ]
          }
        }
      },
      {
        "src": "./src/backed.js",
        "dest": "backed.js",
        "moduleName": "Backed",
        "format": "es",
        "plugins": {
          "babel": {
            "presets": [
              "es2016"
            ]
          }
        }
      }
    ]
  },
  "esdoc": {
    "source": "src/",
    "destination": "docs/"
  }
}
